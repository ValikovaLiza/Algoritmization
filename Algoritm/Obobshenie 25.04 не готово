using System;
class Massive {
    public Massive(string n) {
        string Name = n;
    }
    public string Name { get; set; }
}
class Friend : Massive {
    public Friend(string n) :
        base(n){}}

class PhoneList<T> where T : Massive {
    T[] phList;
    int end;
    public PhoneList() {
        phList = new T[10];
        end = 0;     
    }
        
// Добавить элемент в список.
    public bool Add(T newEntry) {
        if(end == 10) return false;
        phList[end] = newEntry;
        end++;
        return true;
    }
    
    // Найти и возвратить сведения о телефоне по заданному имени.
    public T FindByName(int index) {
        Console.WriteLine(phList[1]);
        return phList[index];}
        }

class UseBaseClassConstraint {
    static void Main() {
        PhoneList<Friend> plist = new PhoneList<Friend>();
        plist.Add(new Friend("Том"));
        plist.Add(new Friend("Гари"));
        plist.Add(new Friend("Матт"));
        Friend frnd = plist.FindByName(2);
        Console.Write(frnd);
 
    }
 }
