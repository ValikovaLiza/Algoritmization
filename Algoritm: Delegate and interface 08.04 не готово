Написать программу которая будет иметь список делегатов для мат операций + - * / 
Пользователь выбирает олперацию и ввести 2 числа. После этого программа вызывает соотвествующий делегат 
и выводит результата на экран. В программе должен присутствовать интерфейс Imath который содержит 
методды для всех 4 мат операций.
Интерфейс работает, делегат - нет
using System;
class HelloWorld {
public interface IMath{
    double Multiplication(double a,double b);
    double Dividing(double a,double b);
    double Addition(double a,double b);
    double Subtraction(double a,double b);
}

public delegate double Operation(double a, double b);

class math: IMath{ 
    double IMath.Multiplication(double a,double b)
    {
        return a * b;
    }
    double IMath.Dividing(double a,double b)
    {
        if (b==0) Console.WriteLine("Who");
        return a*Math.Pow(b,-1);
    }
    double IMath.Addition(double a,double b)
    {
        return a + b;
    }
    double IMath.Subtraction(double a,double b)
    {
        return a - b;
    }
}

  static void Main() {
    Console.WriteLine("Меню");
    Console.WriteLine("1. Умножение");
    Console.WriteLine("2. Деление");
    Console.WriteLine("3. Сложение");
    Console.WriteLine("4. Вычитание");
    int s=Convert.ToInt32(Console.ReadLine());
    double a=Convert.ToDouble(Console.ReadLine()),b=Convert.ToDouble(Console.ReadLine());
    var math_for_us = new math();
    Operation op1 = new(IMath.Multiplication);
    double result=op1(a,b);
    Console.WriteLine(result);
    switch (s){
        case 1: Console.WriteLine(((IMath) math_for_us).Multiplication(a,b));break;
        case 2: Console.WriteLine(((IMath) math_for_us).Dividing(a,b));break;
        case 3: Console.WriteLine(((IMath) math_for_us).Addition(a,b));break;
        case 4: Console.WriteLine(((IMath) math_for_us).Subtraction(a,b));break;
    }
    
  }
}
