f=open("t.txt",'r')
l=[]
s=""
d=""
for i in f:
    l.append(i)
l1=[]
for i in range(len(l)):
    l1=str(l[i]).split()
    print(l1)
    if l1[0]=="DUST":
        s+="DT"
        for j in range(1,len(l1)):
            if l1[j].isdigit()==True:
                s+=d
                s=s.replace(d,'',1)
            s=s+str(l1[j])
            if l1[j].isdigit()==True:
                s=s.replace(l1[j],'',1)
        s+="TD"
        d=s
    elif l1[0]=="MIX":
        s+="MX"
        for j in range(1,len(l1)):
            if l1[j].isdigit()==True:
                s+=d
                s=s.replace(d,'',1)
            s=s+str(l1[j])
            if l1[j].isdigit()==True:
                s=s.replace(l1[j],'',1)
        s+="XM"
    elif l1[0]=="WATER":
        s+="WT"
        for j in range(1,len(l1)):
            if l1[j].isdigit()==True:
                s+=d
                s=s.replace(d,'',1)
            s=s+str(l1[j])
            if l1[j].isdigit()==True:
                s=s.replace(l1[j],'',1)
        s+="TW"
    elif l1[0]=="FIRE":
        s+="FR"
        for j in range(1,len(l1)):
            if l1[j].isdigit()==True:
                s+=d
                s=s.replace(d,'',1)
            s=s+str(l1[j])
            if l1[j].isdigit()==True:
                s=s.replace(l1[j],'',1)
        s+="RF"
    l1=[]
print(s)
DUST XqK DWYMfDBZ ZZdnIqo
FIRE 1 Odg
WATER 1
MIX 3 2 xJZYkKXL
DUST 4 ceeJr
